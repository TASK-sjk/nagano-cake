<!-- # <%= render%> -->

<h1>注文一覧画面</h1>
<table class="table table-striped">
	<thead>
		<tr>
			<td>購入日時</td>
			<td>購入者</td>
			<td>注文個数</td>
			<td>注文ステータス</td>
		</tr>
	</thead>
	<tbody>
		<tr>
		    <% @order each do |order| %>
		    <td><%= l order.created_at %></td>
		    <td><%= order.customer_name %></td>
		    <td><%= order.items.count %></td>
		    <td></td>
	    </tr>

	</tbody>
</table>
<%= paginate @order, class: "paginate" %>