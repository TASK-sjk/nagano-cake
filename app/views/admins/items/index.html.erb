<h2>商品一覧画面</h2>

<div class="container">
	<div class="row">
		<div class="col-xs-10">
			<table class="table">
				<tr>
					<th>商品ID</th>
					<th>商品名</th>
					<th>税抜き価格</th>
					<th>ジャンル</th>
					<th>ステータス</th>
				</tr>
				<% @items.each do |item| %>
				<tr>
					<th><%= item.id %></th>
					<th><%= item.name %></th>
					<th><%= item.price %></th>
					<th></th>
					<th></th>
				</tr>
				<% end %>
			</table>
		</div>
	</div>
</div>